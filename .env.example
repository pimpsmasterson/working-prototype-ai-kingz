# AI KINGS - Environment Variables Template
# Copy this file to .env and fill in your actual keys.
# NEVER commit your actual .env file to version control.

# --- VAST.AI CONFIGURATION ---
# Your Vast.ai API Key from https://cloud.vast.ai/account/
VASTAI_API_KEY=your_vastai_api_key_here

# --- SERVER AUTHENTICATION ---
# Secure key for accessing admin endpoints
ADMIN_API_KEY=secure_admin_key_change_me
# Salt for HMAC-based audit logging (keep this consistent)
AUDIT_SALT=ai_kings_audit_salt_2026_random_entropy

# --- MODEL DOWNLOAD TOKENS ---
# Required for gated models on Hugging Face (e.g. Flux, Wan 2.2)
HUGGINGFACE_HUB_TOKEN=your_hf_token_here
# Required for Civitai downloads
CIVITAI_TOKEN=your_civitai_token_here

# --- PROVISIONING CONFIGURATION ---
# Base URL for modular scripts (e.g. GitHub raw URL)
SCRIPTS_BASE_URL=https://raw.githubusercontent.com/username/repo/main/scripts/
# The main entry point script URL (points to the modular setup.sh)
COMFYUI_PROVISION_SCRIPT=https://gist.githubusercontent.com/pimpsmasterson/5a3dc3d4b9151081f3dab111d741a1e7/raw

# --- SERVER SETTINGS ---
PORT=3000
WARM_POOL_SAFE_MODE=0
WARM_POOL_IDLE_MINUTES=15
COMFYUI_TUNNEL_URL=http://localhost:8188
