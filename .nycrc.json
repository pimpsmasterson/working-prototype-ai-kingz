{
  "all": true,
  "check-coverage": true,
  "lines": 80,
  "functions": 80,
  "branches": 75,
  "statements": 80,
  "reporter": ["html", "text", "lcov"],
  "exclude": [
    "tests/**",
    "assets/js/**",
    "server/db_inspect.js",
    "ecosystem.config.js",
    "coverage/**",
    ".nyc_output/**",
    "node_modules/**"
  ],
  "include": [
    "server/**/*.js"
  ],
  "cache": false,
  "temp-dir": "./.nyc_output"
}
