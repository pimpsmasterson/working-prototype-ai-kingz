{
  "name": "vastai-warm-pool-proxy",
  "version": "1.0.0",
  "description": "Vast.ai warm pool proxy with admin controls and audit logging",
  "main": "server/vastai-proxy.js",
  "scripts": {
    "start": "node server/vastai-proxy.js",
    "dev": "node server/vastai-proxy.js",
    "start:pm2": "pm2 start ecosystem.config.js",
