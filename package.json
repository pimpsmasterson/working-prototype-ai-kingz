    "express": "^4.21.2",
    "node-fetch": "^2.7.0"
  },
  "devDependencies": {
    "chai": "^4.3.8",
    "mocha": "^10.2.0",
    "nock": "^13.3.2",
    "nyc": "^17.1.0",
    "pm2": "^5.2.0",
    "sinon": "^15.2.0",
    "supertest": "^6.3.4"
